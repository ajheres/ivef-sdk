									<xs:maxLength value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="CountryOwner" use="optional">
							<xs:annotation>
								<xs:documentation>The country flag (ISO 3166-1-alpha2)</xs:documentation>
							</xs:annotation>
