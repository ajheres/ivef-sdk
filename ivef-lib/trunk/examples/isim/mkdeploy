NAME=`java -classpath target/luky-small.jar luky.Version | sed 's/ /_/g' |  sed 's/(//g' | sed 's/)//g'`
zip -9r ${NAME}.zip target/* lib/substance.jar 

SRC=${NAME}-src.zip
zip -9r $SRC target/* lib/* beautify mk* licence src/* doc-*

WWW=${NAME}-www.zip
cd htdoc
zip -9r ../$WWW *

PACK=${NAME}-pkg.zip
cd ../package
zip -9r ../$PACK luky.pkg

